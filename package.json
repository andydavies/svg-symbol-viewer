{
  "name": "svg-symbol-viewer",
  "private": true,
  "author": {
    "name": "Jayden Seric",
    "email": "me@jaydenseric.com",
    "url": "https://jaydenseric.com"
  },
  "engines": {
    "node": ">=8.6"
  },
  "browserslist": "> 0.5%, not dead, node 8.6",
  "dependencies": {
    "babel-plugin-transform-inline-environment-variables": "^0.4.3",
    "dotenv-cli": "^1.4.0",
    "next": "^7.0.2",
    "react": "^16.6.3",
    "react-dom": "^16.6.3"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.1",
    "eslint": "^5.9.0",
    "eslint-config-env": "^2.0.0",
    "eslint-config-prettier": "^3.3.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-import-order-alphabetical": "0.0.1",
    "eslint-plugin-node": "^8.0.0",
    "eslint-plugin-prettier": "^3.0.0",
    "eslint-plugin-react": "^7.11.0",
    "prettier": "^1.15.2"
  },
  "scripts": {
    "dev": "dotenv next",
    "build": "dotenv next build",
    "start": "dotenv next start",
    "test": "eslint . && prettier '**/*.{json,yml,md}' -l"
  },
  "eslintConfig": {
    "extends": [
      "env"
    ],
    "rules": {
      "require-jsdoc": "off"
    }
  },
  "prettier": {
    "proseWrap": "never",
    "singleQuote": true,
    "semi": false
  },
  "babel": {
    "presets": [
      "next/babel"
    ],
    "plugins": [
      "transform-inline-environment-variables"
    ]
  },
  "now": {
    "alias": "svgsymbolviewer.io",
    "env": {
      "NODE_ENV": "production",
      "DOMAIN": "https://svgsymbolviewer.io",
      "PORT": "80",
      "GA_TRACKING_ID": "UA-83641101-1"
    }
  }
}
